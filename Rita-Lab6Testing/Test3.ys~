# Revised Program 3
    irmovq $0, %rax          # Load immediate 0 into %rax
    irmovq $10, %rbx         # Load immediate 10 into %rbx

.loop:                       # Define loop start before using it
    addq %rbx, %rax          # Increment %rax by %rbx
    rrmovq %rax, %rcx        # Move %rax to %rcx
    irmovq $100, %rdx        # Load immediate 100 into %rdx

    cmovl %rbx, %rcx         # If %rax is less, move %rbx to %rcx
    jge .end                 # If %rax is greater or equal to %rdx, jump to .end
    jmp .loop                # Loop back to start of .loop

.end:                        # Define end before it's referenced
    halt                     # Stop the program
