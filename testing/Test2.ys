.pos 0
irmovq $42, %rbx       // Load immediate value 42 into %rbx
irmovq $123, %rax      // Load immediate value 123 into %rax
rrmovq %rax, %rcx      // Move value from %rax to %rcx
rmmovq %rax, 8(%rbx)   // Store value from %rax into memory at address (%rbx + 8)
mrmovq 8(%rbx), %rdx   // Load value from memory at address (%rbx + 8) into %rdx
addq %rcx, %rdx        // Add values in %rcx and %rdx, result in %rdx
xorq %rcx, %rcx        // Clear %rcx to set ZF flag (Zero Flag)
irmovq $1, %rsi        // Load immediate value 1 into %rsi
subq %rcx, %rdx        // Subtract %rcx from %rdx, affects flags for next conditional move
halt                   // Halt the program
